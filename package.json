{
  "name": "@promptbook/core",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ui": {
      "import": "./dist/ui/index.js",
      "require": "./dist/ui/index.js",
      "types": "./dist/ui/index.d.ts"
    },
    "./sync": {
      "import": "./dist/sync/index.js",
      "require": "./dist/sync/index.js",
      "types": "./dist/sync/index.d.ts"
    },
    "./kernel": {
      "node": {
        "import": "./dist/kernel/index.js",
        "require": "./dist/kernel/index.js",
        "types": "./dist/kernel/index.d.ts"
      },
      "default": "./dist/kernel/index.js"
    },
    "./utils": {
      "node": {
        "import": "./dist/utils/index.js",
        "require": "./dist/utils/index.js",
        "types": "./dist/utils/index.d.ts"
      },
      "default": "./dist/utils/index.js"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./styles": "./src/ui/styles/index.css"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --max-warnings 10"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@anthropic-ai/claude-agent-sdk": "^0.2.0",
    "@anthropic-ai/sdk": "^0.30.0",
    "zeromq": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/claude-agent-sdk": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "zeromq": {
      "optional": true
    }
  },
  "devDependencies": {
    "@monaco-editor/react": "^4.6.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jsdom": "^23.2.0",
    "monaco-editor": "^0.45.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vitest": "^1.2.0",
    "@anthropic-ai/claude-agent-sdk": "^0.2.0",
    "@anthropic-ai/sdk": "^0.30.0",
    "zeromq": "^6.0.0"
  }
}
